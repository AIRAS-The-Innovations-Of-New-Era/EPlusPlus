# Comprehensive test of all newly implemented built-in functions
print("=== Comprehensive Built-in Functions Test ===")

# ASCII representation
print("\n--- ASCII Function ---")
print("ascii('Hello'):", ascii('Hello'))
print("ascii('Hello\\nWorld'):", ascii('Hello\nWorld'))

# Breakpoint (commented out for automated testing)
# print("\n--- Breakpoint Function ---")
# breakpoint()

# Byte sequences
print("\n--- Byte Sequences ---")
ba = bytearray("Hello")
print("bytearray('Hello'):", ba)
b = bytes("World", "utf-8")
print("bytes('World'):", b)

# Callable check
print("\n--- Callable Function ---")
print("callable(42):", callable(42))
print("callable('hello'):", callable('hello'))

# Directory listing
print("\n--- Dir Function ---")
dir_result = dir('hello')
print("dir('hello') length:", len(dir_result))
dir_result2 = dir([1,2,3])
print("dir([1,2,3]) length:", len(dir_result2))

# Division and modulus
print("\n--- Divmod Function ---")
print("divmod(17, 5):", divmod(17, 5))
print("divmod(17.5, 3.2):", divmod(17.5, 3.2))

# Enumerate
print("\n--- Enumerate Function ---")
items = ['a', 'b', 'c']
enum_result = enumerate(items)
print("enumerate(['a', 'b', 'c']) created")
enum_result2 = enumerate(items, 1)
print("enumerate(['a', 'b', 'c'], 1) created")

# Eval (simplified)
print("\n--- Eval Function ---")
print("eval('42'):", eval('42'))
print("eval('3.14'):", eval('3.14'))

# Format
print("\n--- Format Function ---")
print("format(3.14159, '.2f'):", format(3.14159, '.2f'))
print("format(42):", format(42))

# Hash
print("\n--- Hash Function ---")
print("hash(42):", hash(42))
print("hash('hello'):", hash('hello'))

# Help (basic test)
print("\n--- Help Function ---")
help('print')

# ID function
print("\n--- ID Function ---")
x = 42
print("id(x):", id(x))

# Type checking
print("\n--- Type Checking ---")
print("isinstance(42, 'int'):", isinstance(42, 'int'))
print("isinstance('hello', 'str'):", isinstance('hello', 'str'))
print("issubclass('int', 'int'):", issubclass('int', 'int'))

# Object creation
print("\n--- Object Creation ---")
obj = object()
print("object():", obj)

# Repr function
print("\n--- Repr Function ---")
print("repr('hello'):", repr('hello'))
print("repr([1, 2, 3]):", repr([1, 2, 3]))
print("repr(42):", repr(42))

# Slice object
print("\n--- Slice Object ---")
s = slice(1, 5, 2)
print("slice(1, 5, 2):", s)

# Zip function
print("\n--- Zip Function ---")
list1 = [1, 2, 3]
list2 = ['a', 'b', 'c']
zip_result = zip(list1, list2)
print("zip([1,2,3], ['a','b','c']) created")

print("\n=== All tests completed! ===")