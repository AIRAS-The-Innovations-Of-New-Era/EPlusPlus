# Test: Inheritance and method override
class Animal:
    def speak(self):
        return "..."

class Dog(Animal):
    def speak(self):
        return "woof"

class Cat(Animal):
    def speak(self):
        return "meow"

a = Animal()
d = Dog()
c = Cat()
print(a.speak())  # ...
print(d.speak())  # woof
print(c.speak())  # meow
